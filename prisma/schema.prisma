datasource db {
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Ticket {
    id String @id @default(cuid())
    createAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    title String
    content String @db.VarChar(1204)
}